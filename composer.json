{
    "name": "paksonenko/stars-rating",
    "description": "WP plugin for comments with avarage rating from API's endpoint",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-only",
    "authors": [{
        "name": "Petro Aksonenko",
        "email": "akc.petia92@gmail.com"
    }],
    "keywords": [
        "wordpress"
    ],
    "config": {
        "platform": {
            "php": "7.2"
        },
        "optimize-autoloader": true
    },
    "repositories": [{
        "type": "git",
        "url": "https://github.com/paksonenko/wp-stars-rating"
    }],
    "require": {
        "php": ">=7.2.0",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "php": ">=7.2.0",
        "wp-coding-standards/wpcs": "^2.2",
        "phpcompatibility/php-compatibility": "^9.0",
        "phpcompatibility/phpcompatibility-wp": "^2.1",
        "wordpress/wordpress": "~5.7.0",
        "php-stubs/wordpress-stubs": "~5.7.0"
    },
    "autoload-dev": {
        "classmap": [
            "wordpress/src/"
        ]
    },
    "autoload": {
        "classmap": [
            "stars-rating.php",
            "includes/stars-rating-include.php"
        ]
    },
    "extra": {
        "installer-paths": {
            "{$name}": [
                "wordpress/wordpress"
            ]
        }
    }
}